{
  "name": "unplugin-dbg",
  "type": "module",
  "version": "0.0.0-alpha.6",
  "description": "Rust's `dbg!` for JavaScript, logging values with context effortlessly.",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "swc",
    "debug"
  ],
  "packageManager": "yarn@4.8.1",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./farm": {
      "import": "./dist/farm.js",
      "require": "./dist/farm.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./runtime": {
      "import": {
        "types": "./dist/runtime/dbg.d.ts",
        "default": "./dist/runtime/dbg.js"
      },
      "require": {
        "types": "./dist/runtime/dbg.d.cts",
        "default": "./dist/runtime/dbg.cjs"
      }
    },
    "./runtime-shim": {
      "import": {
        "types": "./dist/runtime/dbg-shim.d.ts",
        "default": "./dist/runtime/dbg-shim.js"
      },
      "require": {
        "types": "./dist/runtime/dbg-shim.d.cts",
        "default": "./dist/runtime/dbg-shim.cjs"
      }
    },
    "./swc-plugin": "./plugin/swc_plugin_dbg.wasm",
    "./globals": "./globals.d.ts",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "globals.d.ts",
    "plugin/swc_plugin_dbg.wasm"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "demo": "yarn build && tsx demo/index.ts",
    "lint": "yarn lint:plugin && yarn lint:runtime",
    "test": "yarn test:plugin && yarn test:runtime",
    "build": "yarn build:plugin && yarn build:runtime",
    "lint:plugin": "cd plugin && cargo fmt --check",
    "test:plugin": "cd plugin && cargo test --all",
    "build:plugin": "cd plugin && cargo build --target wasm32-wasip1 --release && cp target/wasm32-wasip1/release/swc_plugin_dbg.wasm .",
    "lint:runtime": "oxlint src",
    "test:runtime": "vitest --run",
    "build:runtime": "tsup",
    "release": "release-it"
  },
  "dependencies": {
    "es-toolkit": "^1.34.1",
    "unplugin": "^2.1.2"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.16.2",
    "@swc/core": "^1.11.13",
    "@types/node": "^22",
    "esbuild": "^0.25.2",
    "oxlint": "^0.16.4",
    "release-it": "^18.1.2",
    "rollup": "^4.34.6",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.1.0",
    "vitest": "^3.1.1",
    "webpack": "^5.97.1"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "@swc/core": ">=1",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  }
}
